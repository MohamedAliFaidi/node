{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/dist/**",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "index.js"
        }
    ],
    "headers": [
        {
          "key": "default-src",
          "value": "https://node-sage-six.vercel.app"
        }
      ]
    
}