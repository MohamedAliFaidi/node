{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "api/.*",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  