{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/dist/**",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "index.js"
        }
    ],
    "headers": [{
        "Content-Security-Policy": "default-src 'https://node-sage-six.vercel.app'; connect-src 'https://node-sage-six.vercel.app' ;"
    }]
}