{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/dist/**",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "index.js"
      }
      ,  {
        "src": "/.*",
        "dest": "public/dist/index.html"
      }
    ]
  }
  